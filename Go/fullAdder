1, ............ [0] A 
1, ............ [1] B
0, ............ [2] Cin
XorGate,0,1, .. [3] Xor1 <- A,B
XorGate,2,3, .. [4] Xor2 <- Cin, Xor1
AndGate,2,3, .. [5] And1 <- Cin, Xor1 
AndGate,0,1, .. [6] And2 <- A,B
OrGate,5,6, ... [7] Or1 <- And1, And2
Output,4, ..... [8] Sum <- Xor2
Output,7, ..... [9] Cout <- Or1
